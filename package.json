{
  "name": "latchkey",
  "version": "0.1.0",
  "description": "A CLI tool that injects API credentials into curl requests for known third-party services",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "latchkey": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src tests scripts",
    "lint:fix": "eslint src tests scripts --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "start": "node dist/cli.js"
  },
  "keywords": [
    "cli",
    "curl",
    "api",
    "credentials",
    "authentication",
    "oauth"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "playwright": "^1.49.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.0.0",
    "eslint": "^9.39.2",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.53.1",
    "vitest": "^2.0.0"
  }
}
